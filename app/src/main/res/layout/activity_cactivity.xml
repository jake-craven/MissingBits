<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_cactivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.colummullally.missing_bits.Cactivity">


    <TextView
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:id="@+id/p1Text"
        android:layout_below="@+id/PText"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_below="@+id/p4Text"
        android:layout_alignLeft="@+id/p4Text"
        android:layout_alignStart="@+id/p4Text"
        android:layout_marginTop="17dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p4Text"
        android:layout_below="@+id/p3Text"
        android:layout_alignLeft="@+id/p3Text"
        android:layout_alignStart="@+id/p3Text"
        android:layout_marginTop="11dp"
        android:text="" />

    <TextView
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p2Text"
        android:layout_below="@+id/p1Text"
        android:layout_alignLeft="@+id/p1Text"
        android:layout_alignStart="@+id/p1Text"
        android:layout_marginTop="13dp" />

    <TextView
        android:text="Player"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/PText"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p3Text"
        android:text=""
        android:layout_below="@+id/p2Text"
        android:layout_alignLeft="@+id/p2Text"
        android:layout_alignStart="@+id/p2Text"
        android:layout_marginTop="13dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p5Text"
        android:text=""
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toRightOf="@+id/textView5"
        android:layout_toEndOf="@+id/textView5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p6Text"
        android:text=""
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignStart="@+id/textView5"
        android:layout_marginTop="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p7Text"
        android:text=""
        android:layout_below="@+id/p6Text"
        android:layout_alignLeft="@+id/p6Text"
        android:layout_alignStart="@+id/p6Text"
        android:layout_marginTop="18dp" />

    <TextView
        android:text=""
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/p8Text"
        android:layout_below="@+id/p7Text"
        android:layout_alignLeft="@+id/p7Text"
        android:layout_alignStart="@+id/p7Text"
        android:layout_marginTop="27dp" />

    <Button
        android:text="All in"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Abtn"
        android:layout_below="@+id/p8Text"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="74dp" />

    <TextView
        android:text="Room Code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Rcode"
        android:layout_below="@+id/p8Text"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp" />
</RelativeLayout>
